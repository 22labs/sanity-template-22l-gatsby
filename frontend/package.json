    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "prettier:check": "prettier --check './**/*.{js,ts,tsx}'",
    "prettier:fix": "prettier --write './**/*.{js,ts,tsx}'",
    "serve": "gatsby serve",
    "start": "gatsby develop --host 0.0.0.0 --port 8000",
